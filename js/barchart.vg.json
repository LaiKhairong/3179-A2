{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A horizontal bar chart showing unemployment rates by country.",
    "data": {
      "url": "https://raw.githubusercontent.com/LaiKhairong/3179-A2/main/data/Unenployment_rate.csv"
    },
    "mark": "bar",
    "encoding": {
      "y": {
        "field": "Country",
        "type": "nominal",
        "sort": "-x",
        "axis": {"title": "Country"}
      },
      "x": {
        "field": "Unemployment rate",
        "type": "quantitative",
        "axis": {"title": "Unemployment Rate (%)"}
      }
    }
  }
  